    "express": "^4.16.3",
    "material-design-icons": "^3.0.1",
    "material-icons": "^0.2.3",
    "prop-types": "^15.6.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
